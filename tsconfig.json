{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*", "App.tsx", "index.ts"],
  "exclude": ["node_modules", "build", "dist", ".expo"],
  "extends": "expo/tsconfig.base"
}
